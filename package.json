{
	"name": "cppkies",
	"version": "0.2.3",
	"description": "Cppkies is a modding framework for the game Cookie Clicker. It intends to make modding the game very simple.",
	"main": "./dist/index.js",
	"module": "./dist/esm/index5.js",
	"typings": "./dist/index.d.ts",
	"scripts": {
		"test": "npx jest",
		"code-test": "npm run prodBuild && npm run eslint",
		"build": "npx rollup -c",
		"prodBuild": "rm dist -rf && echo {\\\"prod\\\":true} > isprod.json && export NODE_ENV=\"production\" && npm run build && npm run build-dts",
		"watch": "echo {\\\"prod\\\":false} > isprod.json && export NODE_ENV=\"development\"&&npm run build -- --watch",
		"eslint": "npx eslint ./src/**/*.ts ./src/*.ts ./*.js",
		"eslint:fix": "npm run eslint -- --fix",
		"build-dts": "npx dts-bundle-generator src/index.ts -o dist/index.d.ts",
		"watch-esm": "echo {\\\"prod\\\":false} > isprod.json && export NODE_ENV=\"development\" CPPKIES_ESM_DEBUGGING=\"yes\" && npm run build -- --watch"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run code-test && git add .",
			"pre-push": "npm run test"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Cppkies-Team/Cppkies.git"
	},
	"author": "Cppkies team",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Cppkies-Team/Cppkies/issues"
	},
	"types": "./dist/index.d.ts",
	"homepage": "https://github.com/Cppkies-Team/Cppkies#readme",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@types/cookieclicker": "^2.31.5",
		"@types/jest": "^26.0.15",
		"@types/lz-string": "^1.3.34",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"dotenv": "^8.2.0",
		"dts-bundle-generator": "^5.6.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"jest-playwright-preset": "^1.4.6",
		"playwright": "^1.14.1",
		"prettier": "^1.19.1",
		"rollup": "^1.32.1",
		"rollup-plugin-analyzer": "^3.3.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.25.3",
		"serve": "^11.3.2",
		"ts-jest": "^26.4.4",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	},
	"sideEffects": [
		"./dist/esm/onLoad.js"
	]
}
